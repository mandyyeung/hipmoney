<div class="col-md-4">
                        <!--widget start-->
  <aside class="profile-nav alt">
    <header class="panel-heading"><span class='stock title'>My Stock Watchlist</span>
      <input id='input'>
        <span class="tools pull-right">
            <a href="javascript:;" class="fa fa-plus"></a>
        </span>
      </header>
      <section class="panel">
        <div id='results'></div>
          <div class="user-heading alt gray-bg">
              <a >
                 <div class='stock-logo'>
                  <img alt="" src=<%= current_user.stocks[-1].logo || 'https://encrypted-tbn3.gstatic.com/images?q=tbn:ANd9GcREagc_JNfLZ2y2iNotwwik1uduhiCB9HRD0QPVECZK0uqtutIAUA'%>>
                </div>
              </a>
              <h2><%= current_user.stocks[-1].ticker %></h2>
              <p><%= current_user.stocks[-1].name %></p>
          </div>
        <div class="stock-body stocklist">
        <ul class="nav nav-pills nav-stacked">

        <% quotes = current_user.get_price %>
        <% rs = current_user.stocks.reverse %>
        <% rs.each_index do |i| %>
          <li data-name=<%= rs[i].name %> data-ticker=<%= rs[i].ticker %> data-url=<%= rs[i].logo || 'https://encrypted-tbn3.gstatic.com/images?q=tbn:ANd9GcREagc_JNfLZ2y2iNotwwik1uduhiCB9HRD0QPVECZK0uqtutIAUA' %> data-id=<%= rs[i].id %>>
            <span><a>
              <%= rs[i].ticker %>
            </a></span>
            <span class="badge label-success pull-right r-activity">$<%= quotes[i].ask %></span>&nbsp;
            <span><%= link_to "", stocks_path(rs[i]), method: :delete, remote: true, class: "fa fa-minus-circle red" %></span>
          </li>
            <% end %>
          </ul>
        </div>
      </section>
  </aside>
  <!--widget end-->

</div>
